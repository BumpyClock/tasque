{
  "name": "@bumpyclock/tasque",
  "version": "0.2.0",
  "description": "Simple local-first task tracker. Git-friendly. No DB.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BumpyClock/tasque.git"
  },
  "bin": {
    "tsq": "bin/tsq.js"
  },
  "files": [
    "bin/tsq.js",
    "scripts/postinstall.js"
  ],
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "bump:patch": "node scripts/bump-version.js patch",
    "bump:minor": "node scripts/bump-version.js minor",
    "bump:major": "node scripts/bump-version.js major",
    "bump:set": "node scripts/bump-version.js"
  },
  "preferUnplugged": true,
  "optionalDependencies": {
    "@bumpyclock/tasque-darwin-arm64": "0.2.0",
    "@bumpyclock/tasque-darwin-x64": "0.2.0",
    "@bumpyclock/tasque-linux-x64-gnu": "0.2.0",
    "@bumpyclock/tasque-linux-arm64-gnu": "0.2.0",
    "@bumpyclock/tasque-win32-x64-msvc": "0.2.0",
    "@bumpyclock/tasque-win32-arm64-msvc": "0.2.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
